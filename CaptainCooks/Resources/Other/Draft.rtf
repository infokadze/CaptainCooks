    static func getSlots(countOfItem: Int, countOfRorwsInTable: Int) ->  [[Int]] \{\
        var slots: [[Int]] = []\
               \
        for _ in 0..<(countOfItem) \{\
            var array: [Int] = []\
            \
            for number in 0..<(countOfRorwsInTable / 2) \{\
                array.append(number)\
            \}\
            \
            array += array\
            slots.append(array.shuffled())\
        \}\
        return slots\
    \}    \
